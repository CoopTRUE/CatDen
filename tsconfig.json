{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    ".eslintrc.cjs",
    ".prettierrc.cjs",
    "postcss.config.cjs",
    "vite.config.ts",
    "svelte.config.js",
    "src/**/*"
  ]
}
